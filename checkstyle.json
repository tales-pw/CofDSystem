{
  "defaultSeverity": "INFO",
  "extendsConfigPath": "",
  "numberOfCheckerThreads": 5,
  "baseDefines": [],
  "defineCombinations": [],
  "checks": [
    {
      "type": "Anonymous",
      "props": {}
    },
    {
      "type": "AvoidStarImport",
      "props": {}
    },
    {
      "type": "BlockBreakingConditional",
      "props": {}
    },
    {
      "type": "CodeSimilarity",
      "props": {
        "severityIdentical": "WARNING",
        "thresholdIdentical": 60,
        "thresholdSimilar": 120
      }
    },
    {
      "type": "ConstantName",
      "props": {
        "format": "^[A-Z][A-Z0-9]*(_[A-Z0-9_]+)*$",
        "tokens": [
          "INLINE"
        ],
        "ignoreExtern": false
      }
    },
    {
      "type": "ConstantName",
      "props": {
        "format": "^[A-Z][A-Z0-9]*(_[A-Z0-9_]+)*$",
        "tokens": [
          "NOTINLINE"
        ],
        "ignoreExtern": false
      }
    },
    {
      "type": "CyclomaticComplexity",
      "props": {
        "thresholds": [
          {
            "severity": "WARNING",
            "complexity": 16
          },
          {
            "severity": "ERROR",
            "complexity": 26
          }
        ]
      }
    },
    {
      "type": "DocCommentStyle",
      "props": {
        "severity": "IGNORE",
        "startStyle": "twostars",
        "lineStyle": "onestar"
      }
    },
    {
      "type": "Dynamic",
      "props": {
        "severity": "IGNORE"
      }
    },
    {
      "type": "EmptyLines",
      "props": {
        "max": 1,
        "allowEmptyLineAfterSingleLineComment": true,
        "allowEmptyLineAfterMultiLineComment": true,
        "requireEmptyLineAfterPackage": false,
        "requireEmptyLineAfterClass": false,
        "requireEmptyLineAfterInterface": false,
        "requireEmptyLineAfterAbstract": false
      }
    },
    {
      "type": "EmptyPackage",
      "props": {
        "enforceEmptyPackage": true
      }
    },
    {
      "type": "ExtendedEmptyLines",
      "props": {
        "max": 1,
        "skipSingleLineTypes": true,
        "defaultPolicy": "upto",
        "ignore": [],
        "none": [
          "beforePackage",
          "betweenImports",
          "beforeUsing",
          "typeDefinition",
          "afterLeftCurly",
          "beforeRightCurly"
        ],
        "exact": [],
        "upto": [],
        "atleast": []
      }
    },
    {
      "type": "FieldDocComment",
      "props": {
        "severity": "IGNORE",
        "tokens": [
          "ABSTRACT_DEF",
          "CLASS_DEF",
          "ENUM_DEF",
          "INTERFACE_DEF",
          "TYPEDEF_DEF"
        ],
        "fieldType": "FUNCTIONS",
        "modifier": "PUBLIC",
        "requireParams": false,
        "requireReturn": false,
        "excludeNames": [
          "new",
          "toString"
        ],
        "ignoreOverride": true
      }
    },
    {
      "type": "Final",
      "props": {}
    },
    {
      "type": "Indentation",
      "props": {
        "severity": "IGNORE",
        "character": "    ",
        "ignoreConditionals": false,
        "ignoreComments": true,
        "wrapPolicy": "larger",
        "conditionalPolicy": "ignore"
      }
    },
    {
      "type": "IndentationCharacter",
      "props": {
        "character": "space",
        "ignorePattern": "^$"
      }
    },
    {
      "type": "Interface",
      "props": {
        "allowMarkerInterfaces": true,
        "allowProperties": false
      }
    },
    {
      "type": "LineLength",
      "props": {
        "max": 140,
        "ignorePattern": "^$"
      }
    },
    {
      "type": "MethodCount",
      "props": {
        "maxTotal": 15,
        "maxPrivate": 110,
        "maxPublic": 110
      }
    },
    {
      "type": "MethodLength",
      "props": {
        "max": 30,
        "ignoreEmptyLines": true
      }
    },
    {
      "type": "NestedControlFlow",
      "props": {
        "max": 3
      }
    },
    {
      "type": "NestedIfDepth",
      "props": {
        "max": 1
      }
    },
    {
      "type": "NullableParameter",
      "props": {
        "option": "nullDefault"
      }
    },
    {
      "type": "OperatorWhitespace",
      "props": {
        "assignOpPolicy": "around",
        "unaryOpPolicy": "none",
        "ternaryOpPolicy": "around",
        "arithmeticOpPolicy": "around",
        "compareOpPolicy": "around",
        "bitwiseOpPolicy": "ignore",
        "boolOpPolicy": "around",
        "intervalOpPolicy": "none",
        "arrowPolicy": "ignore",
        "arrowFunctionPolicy": "around",
        "oldFunctionTypePolicy": "ignore",
        "newFunctionTypePolicy": "around"
      }
    },
    {
      "type": "ParameterNumber",
      "props": {
        "max": 5,
        "ignoreOverriddenMethods": false
      }
    },
    {
      "type": "RedundantAccessMeta",
      "props": {
        "prohibitMeta": false
      }
    },
    {
      "type": "RedundantAllowMeta",
      "props": {
        "prohibitMeta": false
      }
    },
    {
      "type": "RedundantModifier",
      "props": {
        "enforcePublicPrivate": false,
        "enforcePublic": false,
        "enforcePrivate": true
      }
    },
    {
      "type": "Return",
      "props": {
        "allowEmptyReturn": true,
        "enforceReturnType": false,
        "enforceReturnTypeForAnonymous": false
      }
    },
    {
      "type": "ReturnCount",
      "props": {
        "max": 4,
        "ignoreFormat": "^$"
      }
    },
    {
      "type": "RightCurly",
      "props": {
        "tokens": [
          "CLASS_DEF",
          "ENUM_DEF",
          "ABSTRACT_DEF",
          "TYPEDEF_DEF",
          "INTERFACE_DEF",
          "OBJECT_DECL",
          "FUNCTION",
          "FOR",
          "WHILE",
          "SWITCH",
          "TRY",
          "CATCH"
        ],
        "option": "aloneorsingle"
      }
    },
    {
      "type": "RightCurly",
      "props": {
        "tokens": [
          "IF"
        ],
        "option": "same"
      }
    },
    {
      "type": "SeparatorWhitespace",
      "props": {
        "dotPolicy": "none",
        "commaPolicy": "after",
        "semicolonPolicy": "ignore",
        "allowTrailingComma": false
      }
    },
    {
      "type": "SeparatorWrap",
      "props": {
        "option": "eol",
        "tokens": [
          ","
        ]
      }
    },
    {
      "type": "Spacing",
      "props": {
        "spaceIfCondition": "should",
        "spaceForLoop": "should",
        "spaceWhileLoop": "should",
        "spaceSwitchCase": "should",
        "spaceCatch": "any",
        "spaceAroundBinop": false,
        "noSpaceAroundUnop": false,
        "ignoreRangeOperator": true
      }
    },
    {
      "type": "StringLiteral",
      "props": {
        "policy": "doubleAndInterpolation",
        "allowException": true
      }
    },
    {
      "type": "TODOComment",
      "props": {
        "severity": "WARNING",
        "format": "^\\s*(TODO|FIXME|HACK|XXX|BUG)"
      }
    },
    {
      "type": "Trace",
      "props": {
        "severity": "ERROR"
      }
    },
    {
      "type": "TrailingWhitespace",
      "props": {}
    },
    {
      "type": "Type",
      "props": {
        "severity": "IGNORE",
        "ignoreEnumAbstractValues": true
      }
    },
    {
      "type": "TypeDocComment",
      "props": {
        "severity": "IGNORE",
        "tokens": [
          "ABSTRACT_DEF",
          "CLASS_DEF",
          "ENUM_DEF",
          "INTERFACE_DEF",
          "TYPEDEF_DEF"
        ]
      }
    },
    {
      "type": "UnnecessaryConstructor",
      "props": {}
    },
    {
      "type": "UnusedImport",
      "props": {
        "ignoreModules": [],
        "moduleTypeMap": {}
      }
    },
    {
      "type": "UnusedLocalVar",
      "props": {}
    }
  ],
  "exclude": {},
  "version": 1
}